# 데이터베이스
MONGODB_URI=mongodb://localhost:27017/wherewemeets

# JWT 시크릿
JWT_SECRET=your-jwt-secret-key

# API 키들
KAKAO_API_KEY=your-kakao-api-key

# Gemini API Keys (여러 키를 콤마로 구분하여 입력 - API 한도 초과 시 자동 로테이션)
# 단일 키: GEMINI_API_KEY=your-gemini-api-key
# 여러 키: GEMINI_API_KEY=key1,key2,key3,key4
GEMINI_API_KEY=your-gemini-api-key

# Google Maps API Key (장소 검색 정확도 향상용)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# 서버 설정
PORT=5000
NODE_ENV=development

# 도메인 설정 (프로덕션)
FRONTEND_URL=https://www.wherewemeets.com
BACKEND_URL=https://wherewemeets-production.up.railway.app

# Stripe (결제)
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# CORS 설정
CORS_ORIGIN=https://www.wherewemeets.com

# NPM 빌드 설정 (Railway용)
NPM_CONFIG_LEGACY_PEER_DEPS=true
CI=false
NODE_OPTIONS=--max-old-space-size=4096

# Model Configuration (선택사항 - 기본값: 2.5-flash)
GEMINI_MODEL=2.5-flash 