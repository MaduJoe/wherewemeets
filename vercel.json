{
  "name": "wherewemeets-client",
  "version": 2,
  "buildCommand": "cd client && npm ci && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "cd client && npm ci",
  "framework": "create-react-app",
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 